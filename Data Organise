Sub GL_183180_CAD()
Dim iLastrow As Long

Application.Calculation = xlCalculationManual
Application.ScreenUpdating = False
Application.DisplayStatusBar = False
Application.EnableEvents = False
ActiveSheet.DisplayPageBreaks = False

iLastrow = CAD183.Cells(CAD183.Cells.Rows.Count, 1).End(xlUp).Row
iLastrow1 = Sheet3.Cells(Sheet3.Cells.Rows.Count, 1).End(xlUp).Row

For i = 1 To iLastrow
    If CAD183.Cells(i, 7) <> "" Then
    CAD183.Cells(i, 8) = CAD183.Cells(i + 1, 1)
    iLastrow1 = Sheet3.Cells(Sheet3.Cells.Rows.Count, 1).End(xlUp).Row
    CAD183.Rows(i).Copy
    Sheet3.Rows(iLastrow1 + 1).PasteSpecial
    Sheet3.Cells(iLastrow1 + 1, 9) = "183-180 CAD"
    End If
Next

Application.Calculation = xlCalculationAutomatic
Application.ScreenUpdating = True
Application.DisplayStatusBar = True
Application.EnableEvents = True
ActiveSheet.DisplayPageBreaks = True
End Sub
